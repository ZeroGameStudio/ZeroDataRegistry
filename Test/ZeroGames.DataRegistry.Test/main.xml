


<Schema Name="MainRegistry" Namespace="ZeroGames.DataRegistry.Test">
    
    <Import Uri="shared" Alias="Shared" />

    <Entity Name="MyConstant" Namespace="Temp">
        <PrimaryKey Type="uint64" Name="Id" />
    </Entity>

    <Entity Name="Item" Namespace="System">
        <PrimaryKey Type="uint64" Name="Id" />
        <Property Type="string" Name="Name" />
        <PrimaryKey Type="string" Name="Uri" />
        <ForeignKey Type="Icon" Name="Icon" />
    </Entity>
    
    <Entity Name="Weapon" Extends="Item" Namespace="System">
        <Property Type="Shared.Point3D" Name="AttachPoint" />
        <Property Type="Shared.EDamageType" Name="DamageType" />
        <Property Type="double" Name="Damage" />
    </Entity>
    
    <Entity Name="Icon">
        <PrimaryKey Type="uint64" Name="Id" />
        <Property Type="string" Name="Path" />
    </Entity>

</Schema>
